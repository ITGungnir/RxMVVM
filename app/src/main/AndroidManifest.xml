<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="my.itgungnir.rxmvvm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup, GoogleAppIndexingWarning">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".app1.AppActivity1" />

        <activity android:name=".app2.AppActivity2" />

        <activity android:name=".app3.AppActivity3" />

        <activity android:name=".app4.AppActivity4" />

        <activity android:name=".app5.AppActivity5" />

        <activity android:name=".app6.AppActivity6" />

        <activity android:name=".redux.NonPersistReduxActivity" />

        <activity android:name=".redux.PersistReduxActivity" />

    </application>

</manifest>
